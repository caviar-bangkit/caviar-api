generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Crossing {
  id        Int      @id @unique @default(autoincrement())
  name      String   @unique
  latitude  Float
  longitude Float
  heading   Float
  created   DateTime @default(now())
  updated   DateTime @default(now()) @updatedAt
}
