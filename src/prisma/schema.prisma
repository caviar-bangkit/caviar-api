generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:090202@127.0.0.1:3308/caviar-db"
}

model Crossing {
  id        Int      @id @unique @default(autoincrement())
  name      String   @unique
  latitude  Float
  longitude Float
  heading   Float
  created   DateTime @default(now())
  updated   DateTime @default(now()) @updatedAt
}
